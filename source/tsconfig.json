{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "isolatedModules": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "noImplicitAny": true,
    "removeComments": false,
    "noLib": false,
    "preserveConstEnums": false,
    "strict": true,
    "suppressImplicitAnyIndexErrors": false,
    "importHelpers": true,
    "outDir": "../lib"
  }
}
